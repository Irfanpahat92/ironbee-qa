    "test6": {
        "description": "Test of SensorId Directive",
        "enabled": "True",
        "normalize":"False",
        "local_apache":{
            "apache_vars" : "@IRONBEE_CONF_TEMPLATE@:@CWD@/tests/ironbee.conf.test6.in"
        },    
        "matches": {
            "file_matches": {
                "server_root/logs/audit/auditlog.log": {
                    "format" : "ironbee_audit_log_index",
                    "re": [
                        "(?sm)^\\s*\\\"log-id\\\"\\:.+98763000\\\"\\,\\s*$"
                    ]
                }
            }
        },
        "raw_request": "GET / HTTP/1.1\r\nHost: foo\r\n\r\n"
    },
